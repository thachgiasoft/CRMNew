                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with A
as
(
	select NhanVien
		, COUNT(KhachHang) as SoKH
		, sum(SoLuot) as SoLuot
	from
	(
	select NhanVien, KhachHang, COUNT(*) as SoLuot
	from TuVan
	where TrangThai=1
	and NgayTrangThai&gt;=@d1 and NgayTrangThai&lt;=@d2
	group by NhanVien, KhachHang
	) as B
	group by NhanVien
)
select NhanVien, HoTen
	,SoKH
	,SoLuot
	, convert(float,SoLuot)/convert(float,SoKH) as TiLe
from A
join NguoiDung on NhanVien=TenDangNhap</CommandText>
