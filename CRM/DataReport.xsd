          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DTBQTheoNhomKHTableAdapter" GeneratorDataComponentClassName="DTBQTheoNhomKHTableAdapter" Name="DTBQTheoNhomKH" UserDataComponentName="DTBQTheoNhomKHTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="CRMConnString (Settings)" DbObjectName="CRM_WIN.dbo.NhomKH" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with DoanhThu
as
(select KhachHang
, sum(TongThanhToan) as TongThanhToan
, count(*) as SoDonHang
from PhieuDatHang
where TrangThai&lt;2 and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
group by KhachHang)
select NKH.ID
, NKH.TenNhom
, ISNULL(sum(TongThanhToan),0) as TongDoanhThu
, ISNULL(sum(SoDonHang),0) as TongDonHang
, (case when ISNULL(sum(SoDonHang),0)=0 then 0 else ISNULL(sum(TongThanhToan),0)/ISNULL(sum(SoDonHang),0) end) as DTBQ
from DoanhThu DT
inner join KhachHang KH on DT.KhachHang=KH.MaKH
right join NhomKH NKH on KH.NhomKH=NKH.ID
group by NKH.ID, NKH.TenNhom</CommandText>
