                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select 
 a.SoPhieu
,a.NgayPhieu
,a.KhachHang
,k.TenKH
,c.SanPham
,s.TenSP
,c.DVT
,c.DonGia
,c.SoLuong
,c.CK
,c.TienCK
,c.ThanhTien
,NVBanHang
,HoTen
from(
	select * from PhieuDatHang p
	where  NVBanHang = @TenDangNhap 
	
	or  (select Loai from NguoiDung where TenDangNhap = @TenDangNhap) &gt; 1 
	or p.SoPhieu in (select SoPhieuDat from TuVan where NhanVien = @TenDangNhap)
	)
as A

inner join CTPhieuDatHang c on a.SoPhieu = c.SoPhieu
inner join KhachHang k on k.MaKH = a.KhachHang
inner join SanPham s on s.MaSP = c.SanPham
inner join NguoiDung us on NVBanHang=us.TenDangNhap

where (cast(NgayPhieu as date) &gt;= @d1 and cast(NgayPhieu as date) &lt;= @d2)</CommandText>
