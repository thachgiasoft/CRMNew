          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SoLanMuaTheoKhachHangTableAdapter" GeneratorDataComponentClassName="SoLanMuaTheoKhachHangTableAdapter" Name="SoLanMuaTheoKhachHang" UserDataComponentName="SoLanMuaTheoKhachHangTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="CRMConnString (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with A
as
(
	select NhomKH, sum(SoKH) as SoKH, sum(SLM) as SLM, sum(SoKH2L) as SoKH2L
	from 
	(
		select NhomKH, count(*) as SoKH, 0 as SLM, 0 as SoKH2L
		from KhachHang
		where CreatedDate&gt;=@d1 and CreatedDate&lt;=@d2
		group by NhomKH
		union all
		select KH.NhomKH, 0 , count(*) as SLM, 0
		from PhieuDatHang DH
		join KhachHang KH on DH.KhachHang=KH.MaKH
		join NhomKH NKH on KH.NhomKH=NKH.ID
		where TrangThai&lt;2
		and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
		group by KH.NhomKH
		union all
		select 
		NhomKH, 0, 0, count(*) as SKH
		from (
		select NhomKH, KhachHang
		from PhieuDatHang DH
		join KhachHang KH on DH.KhachHang=KH.MaKH		
		where TrangThai&lt;2
		and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
		group by KhachHang, NhomKH
		having count(*)&gt;1) as C
		group by NhomKH
	) as B
	group by NhomKH
)
select ID, TenNhom
,SoKH
,SLM
,SoKH2L
,(case when SoKH&gt;0 then convert(float,SLM)*100/SoKH else 0 end) as TiLeMua
,(case when SoKH&gt;0 then convert(float,SoKH2L)*100/SoKH else 0 end) as TiLeMua2Lan
from A
join NhomKH NKH on A.NhomKH=NKH.ID
</CommandText>
