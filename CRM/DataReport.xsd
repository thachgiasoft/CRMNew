          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SoLanMuaTheoKhachHangTableAdapter" GeneratorDataComponentClassName="SoLanMuaTheoKhachHangTableAdapter" Name="SoLanMuaTheoKhachHang" UserDataComponentName="SoLanMuaTheoKhachHangTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="CRMConnString (Settings)" DbObjectName="SELECT ID, TenNhom, COUNT(*) AS SoKH FROM KhachHang KH JOIN NhomKH NKH ON KH.NhomKH=NKH.ID GROUP BY ID,TenNhom" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with SoLanMua
as
(
select ID, TenNhom, count(*) as SoLan
from PhieuDatHang DH
join KhachHang KH on DH.KhachHang=KH.MaKH
join NhomKH NKH on KH.NhomKH=NKH.ID
where TrangThai&lt;2 and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
group by ID, TenNhom)
select B.ID, B.TenNhom
, isnull(SoLan,0) as SoLanMua
, isnull(SoKH,0)  as SoKH
, (case when isnull(SoKH,0)&gt;0 then isnull(SoLan,0)/ISNULL(SoKH,0) else 0 end) as TiLe
from SoLanMua SLM 
right join	(select ID, TenNhom, count(*) as SoKH
		from KhachHang KH
		join NhomKH NKH on KH.NhomKH=NKH.ID
		group by ID,TenNhom) as B on SLM.ID=B.ID</CommandText>
