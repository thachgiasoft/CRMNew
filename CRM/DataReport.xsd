                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with ThongKe
as
(
select NhanVien, sum(SoLanTV) as SoLanTV, sum(SoDH) as SoDH
from
	(select NhanVien, 1 as SoLanTV, 0 as SoDH
	from TuVan
	where TrangThai=1
	and NgayTrangThai&gt;=@d1 and NgayTrangThai&lt;=@d2
	union all
	select NVBanHang,0 as SoLanTV, 1 as SoDH
	from PhieuDatHang
	Where TrangThai&lt;2
	and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
	) as A
group by NhanVien
)
select NhanVien, HoTen
,SoLanTV
,SoDH
,(case when SoLanTV&gt;0 then convert(float,SoDH)*100/convert(float,SoLanTV) else 0 end) as TiLe
from ThongKe
inner join NguoiDung on NhanVien=TenDangNhap</CommandText>
