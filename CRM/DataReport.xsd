                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select a.*,s.TenSP from
(
select 
0 as Loai
,p.NgayPhieu
,p.SoPhieu as SoPhieu
,p.MucDichSD
,p.GhiChu

,c.SanPham
,c.DonGia
,c.DVT
