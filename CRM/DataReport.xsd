                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with TongDoanhThu
as
(select KhachHang, count(*) as SoDH, Sum(TongThanhToan ) as DoanhThu
from PhieuDatHang
where TrangThai&lt;2 and NgayPhieu&gt;=@d1 and NgayPhieu&lt;=@d2
